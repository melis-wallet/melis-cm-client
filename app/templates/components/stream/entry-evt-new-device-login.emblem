
.col-md-12
  .panel.panel-info
    .panel-heading.light
      span.icon: i.fa.fa-chain
      i.text-default = live-moment-from-now entry.updated
    .panel-body
      .row
        .col-xs-3
        .col-xs-9
          .note.note-warning.no-margin-b
            h4  = t 'stream.evt.newd.title'
            p  = t 'stream.evt.newd.caption' label=label id=event.cmo.deviceId  time=(live-moment-from-now  entry.updated)