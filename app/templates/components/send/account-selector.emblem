= power-select [
    options=accounts
    selected=selected
    searchEnabled=false
    placeholder=(t 'paysend.form.account.ph')
    onChange=(action "changedAccount")
] as |c|
  span.label.colored-bg class=c.color style='min-width: 24px; margin-right: 5px;' = account-identifier account=c
  b = c.name
